# creating the dataset

courses = list(data_graph.keys())
values = list(data_graph.values())
  
fig = plt.figure(figsize = (10, 5))
 
# creating the bar plot
plt.bar(courses, values, color ='teal',
        width = 0.4)
 
plt.xlabel("Models")
plt.ylabel("Accuracy")
plt.title("MODELS SCORE ")
plt.show()

